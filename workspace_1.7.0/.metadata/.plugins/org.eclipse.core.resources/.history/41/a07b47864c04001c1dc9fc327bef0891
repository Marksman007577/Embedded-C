#include<stdio.h>
#include<stdint.h>

int main(void)
{
	uint8_t data = 50;

	printf("Value of data is : %d\n",data);
	printf("Address of the variable data is : %p\n", &data);

	uint8_t *pAddress = &data;

	*pAddress = 45;

	printf("Value of data is : %d\n",data);

	return 0;

}//end function main
